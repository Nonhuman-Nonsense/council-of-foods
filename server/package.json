  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "e2e-server": "cross-env NODE_ENV=test node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "proto": "cross-env NODE_ENV=prototype node server.js",
    "protodev": "cross-env NODE_ENV=prototype nodemon server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:fast": "cross-env TEST_MODE=fast vitest run",
    "test:full": "cross-env TEST_MODE=full vitest run",
    "test:coverage": "vitest run --coverage",
    "docker": "docker build ../. -t nonhumannonsense/council-of-foods:latest --platform linux/amd64",
    "push": "docker push nonhumannonsense/council-of-foods:latest"
  },
  "dependencies": {
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongodb": "^6.20.0",
    "openai": "^6.1.0",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.15",
    "mongodb-memory-server": "^10.4.1",
    "nodemon": "^3.1.10",
    "socket.io-client": "^4.8.1",
    "vitest": "^4.0.15"
  }
}